{
    "al.enableCodeAnalysis": true,
    "al.codeAnalyzers": [
        "${CodeCop}",
        "${UICop}"
    ],
    "al.enableCodeActions": true,
    "al.browser": "Chrome",
    "al.incognito": false,
    "al.ruleSetPath": "./.codeAnalysis/thisproject.ruleset.json",
    "CRS.OnSaveAlFileAction": "Rename",
    "editor.formatOnSave": true,
    "CRS.AlSubFolderName": "None",
    "CRS.FileNamePattern": "<ObjectNameShort>.<ObjectTypeShortPascalCase>.al",
    "CRS.FileNamePatternExtensions": "<ObjectNameShort>.<ObjectTypeShortPascalCase>.al",
    "CRS.FileNamePatternPageCustomizations": "<ObjectNameShort>.<ObjectTypeShortPascalCase>.al",
    "CRS.RemovePrefixFromFilename": true,
    "CRS.RemoveSuffixFromFilename": true,
    "CRS.ExtensionObjectNamePattern": "<BaseNameShort>Ext",
    "workbench.colorCustomizations": {
        "titleBar.activeBackground": "#391742",
        "activityBar.background": "#391742",
        "titleBar.activeForeground": "#ffffff",
        "activityBar.foreground": "#ffffff"
    }
}